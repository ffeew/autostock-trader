# Alpaca API Configuration
ALPACA_API_KEY=your_api_key_here
ALPACA_SECRET_KEY=your_secret_key_here
ALPACA_PAPER=true

# Data Configuration
SYMBOLS=SPY,QQQ
# For minute-level data, use recent date range (last 1-6 months recommended)
# Minute data generates ~7,500 bars per symbol per trading day
# 3 months ≈ 1.4M rows, 6 months ≈ 2.8M rows
START_DATE=2024-10-01
END_DATE=2024-12-31
TIMEFRAME=Minute  # Options: Minute, Hour, Day
PREDICTION_STEPS=30


